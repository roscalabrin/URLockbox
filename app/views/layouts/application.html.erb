<!DOCTYPE html>
<html>
  <head>
    <title>URLockbox</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="container">
      <% if current_user %>
        <%= link_to "Sign Out", logout_path, method: :delete %>
      <% end %>
      <div class="col-md-12">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
